{
  "name": "seward-park-map",
  "description": "",
  "version": "0.0.0",
  "repository": {
    "url": "git://github.com/sethvincent/seward-park-map.git"
  },
  "main": "map.js",
  "browserify": {
    "transform": [
      "brfs"
    ]
  },
  "scripts": {
    "parse-csv": "node bin/parse-csv.js",
    "bundle-css": "rework-npm style.css -o assets/bundle.css",
    "bundle-js": "browserify app.js -o assets/bundle.js -t brfs",
    "bundle": "npm run bundle-css & npm run bundle-js & npm run mobile-css & npm run mobile-js",
    "mobile-css": "rework-npm m/m.css -o assets/mobile.css",
    "mobile-js": "browserify m/m.js -o assets/mobile.js -t brfs",
    "mobile": "npm run mobile-css & npm run mobile-js",
    "watch-js": "watchify app.js -o assets/bundle.js -t brfs",
    "watch-css": "gazer -p style.css npm run bundle-css",
    "watch": "npm run watch-js & npm run watch-css",
    "start": "serve -p 3123 & npm run watch"
  },
  "dependencies": {
    "array-filter": "^0.2.0",
    "component-delegate": "^0.2.3",
    "element-class": "^0.1.1",
    "fastclick": "^1.0.1",
    "flatsheet-api-client": "^2.0.0",
    "geolocation-stream": "0.0.1",
    "handlebars": "~1.3.0",
    "hash-match": "^1.0.1",
    "leaflet": "~0.7.2",
    "leaflet-providers": "^1.0.3",
    "mapbox.js": "^2.1.0",
    "normalize-css": "git://github.com/sethvincent/normalize-css",
    "wayfarer": "^2.2.0"
  },
  "devDependencies": {
    "brfs": "^1.2.0",
    "browserify": "^5.11.2",
    "csv-parser": "^1.4.6",
    "gazer": "0.0.3",
    "myth": "~0.3.0",
    "rework-npm-cli": "0.0.1",
    "serve": "^1.4.0",
    "through2": "^0.6.3",
    "through2-map": "^1.4.0",
    "watchify": "^0.8.3"
  }
}
